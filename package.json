{
  "name": "@w11k/git-info",
  "version": "1.1.2",
  "description": "creates a version file to display git info in your angular app",
  "scripts": {
    "schematics": "schematics",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "dev:dry-run": "schematics .:git-info --verbose",
    "dev": "schematics .:git-info --debug=false"
  },
  "keywords": [
    "schematics",
    "angular"
  ],
  "author": "Kai Henzler",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^10.0.6",
    "@angular-devkit/schematics": "^10.0.6",
    "@angular-devkit/schematics-cli": "^12.2.17",
    "typescript": "~3.8.2"
  },
  "devDependencies": {
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "jasmine": "^3.5.0"
  },
  "main": "src/git-info/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w11k/angular-git-info.git"
  },
  "bugs": {
    "url": "https://github.com/w11k/angular-git-info/issues"
  },
  "homepage": "https://github.com/w11k/angular-git-info#readme"
}
